  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "b:dev": "cross-env NODE_ENV=dev&&cross-env BUILD_ENV=development&& node build/build.js",
    "b:prod": "cross-env NODE_ENV=production&&cross-env BUILD_ENV=production&& node build/build.js",
    "b:qa": "cross-env NODE_ENV=qa&&cross-env BUILD_ENV=qa&& node build/build.js",
    "b:pre": "cross-env NODE_ENV=pre&&cross-env BUILD_ENV=pre&& node build/build.js",
    "mock": "json-server --watch mock/data.json",
    "mockdev": "npm run mock & npm start"
  },
